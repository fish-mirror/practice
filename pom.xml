         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.zjicm</groupId>
    <artifactId>practice</artifactId>
    <version>2.0</version>
    <packaging>war</packaging>


    <!-- 依赖版本参数信息 默认值-->
    <properties>
        <spring.version>4.2.7.RELEASE</spring.version>
        <tiles.version>3.0.5</tiles.version>
        <final.name>practice</final.name>
        <default.encoding>UTF-8</default.encoding>
        <compiler.fork>false</compiler.fork>
        <compiler.executable>/opt/java8/bin/javac</compiler.executable>
        <compiler.version>1.8</compiler.version>
        <compiler.source>1.8</compiler.source>
        <compiler.target>1.8</compiler.target>
        <test.skip>true</test.skip>
        <test.jvm>/opt/java8/bin/java</test.jvm>
    </properties>

    <build>

        <finalName>${final.name}</finalName>

        <plugins>

            <!--
                编译设置
                参数说明:https://maven.apache.org/plugins/maven-compiler-plugin/compile-mojo.html
            -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <fork>${compiler.fork}</fork>
                    <executable>${compiler.executable}</executable>
                    <compilerVersion>${compiler.version}</compilerVersion>
                    <source>${compiler.source}</source>
                    <target>${compiler.target}</target>
                    <encoding>${default.encoding}</encoding>
                    <showDeprecation>true</showDeprecation>
                    <showWarnings>true</showWarnings>
                    <verbose>true</verbose>

                    <!--<compilerArguments>-->
                    <!--&lt;!&ndash; 包含原来的lib目录包 &ndash;&gt;-->
                    <!--<extdirs>web/WEB-INF/lib</extdirs>-->
                    <!--</compilerArguments>-->

                </configuration>
            </plugin>

            <!-- 多目录的source配置 http://www.mojohaus.org/build-helper-maven-plugin/usage.html -->
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <version>1.10</version>
                <executions>

                    <!-- 增加资源配置目录 -->
                    <execution>
                        <id>add-resource</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>add-resource</goal>
                            <goal>add-test-resource</goal>
                        </goals>
                        <configuration>
                            <resources>
                                <resource>
                                    <directory>res</directory>
                                </resource>

                            </resources>
                        </configuration>
                    </execution>

                    <!-- 增加源码目录 -->
                    <execution>
                        <id>add-source</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>add-source</goal>
                            <goal>add-test-source</goal>
                        </goals>
                        <configuration>
                            <sources>

                                <source>src</source>

                            </sources>
                        </configuration>
                    </execution>

                </executions>
            </plugin>

            <!-- Test 测试 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.19.1</version>
                <configuration>
                    <!--<skipAfterFailureCount>1</skipAfterFailureCount>-->
                    <skipTests>${test.skip}</skipTests>
                    <jvm>${test.jvm}</jvm>
                    <argLine>-Dfile.encoding=UTF-8 -Dclientname=A -Denvironment=dev -Dserver.role=master</argLine>
                    <excludes>
                        <exclude>**/DCTest.java</exclude>
                    </excludes>
                </configuration>
            </plugin>

            <!-- War 打包配置信息 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-war-plugin</artifactId>
                <version>2.1.1</version>
                <configuration>
                    <warName>${project.build.finalName}</warName>
                    <warSourceDirectory>web</warSourceDirectory>
                    <webXml>web/WEB-INF/web.xml</webXml>
                    <outputDirectory>${project.build.directory}/dist</outputDirectory>

                    <!-- 影响 web 资源（非） -->
                    <!--<warSourceExcludes>-->
                    <!--**/*.java-->
                    <!--</warSourceExcludes>-->

                    <!-- 打包排除 java 文件 -->
                    <packagingExcludes>
                        **/*.java,**/.svn/**
                    </packagingExcludes>
                </configuration>
            </plugin>

            <!-- tomcat 部署插件 http://tomcat.apache.org/maven-plugin-2.1/tomcat7-maven-plugin/run-mojo.html -->
            <!--
                本地测试
                mvn clean -Plocal tomcat7:run（因为本地jvm参数设置原因，会失败，使用 idea maven 配置运行）
                Run/Debug Configurations -> Maven
                -> Parameters Command Line: mvn clean -Plocal tomcat7:run
                -> Runner VM Options: -Dfile.encoding=UTF-8 -Dclientname=A -Denvironment=dev -Dserver.role=master

                配置好后可 Run／Debug
            -->
            <plugin>
                <groupId>org.apache.tomcat.maven</groupId>
                <artifactId>tomcat7-maven-plugin</artifactId>
                <version>2.2</version>
                <configuration>
                    <uriEncoding>${default.encoding}</uriEncoding>
                    <server>aliyun</server>
                    <url>http://139.129.37.224:8080/manager/text</url>
                    <path>/practice</path>
                    <warFile>${project.build.directory}/dist/${project.build.finalName}.${project.packaging}</warFile>
                    <warSourceDirectory>web</warSourceDirectory>
                    <!--<systemProperties>-->
                    <!--<JAVA_OPTS>-Dfile.encoding=UTF-8 -Dclientname=A -Denvironment=dev -Dserver.role=master</JAVA_OPTS>-->
                    <!--</systemProperties>-->
                </configuration>
            </plugin>

        </plugins>
    </build>

